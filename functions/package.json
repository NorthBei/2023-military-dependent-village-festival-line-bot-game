{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts . --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "NODE_ENV=prod npm run build && NODE_ENV=prod firebase functions:shell",
    "start": "npm run shell",
    "deploy:beta": "NODE_ENV=beta firebase deploy --project=beta --only functions",
    "deploy:prod": "NODE_ENV=prod firebase deploy --project=prod --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/functions/src/index.js",
  "dependencies": {
    "@line/bot-sdk": "^7.7.0",
    "dotenv-flow": "^3.3.0",
    "fecha": "^4.2.3",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
